<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.adcoretechnologies.golfbudz">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.SET_DEBUG_APP"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:name=".myapplication.MyApplication"
        android:theme="@style/AppTheme">

        <!--
     Set custom default icon. This is used when no icon is set for incoming notification messages.
     See README(https://goo.gl/l4GJaQ) for more.
        -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/ic_clock" />
        <!--
             Set color used with incoming notification messages. This is used when no color is set for the incoming
             notification message. See README(https://goo.gl/6BKBk7) for more.
        -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />

        <activity
            android:name="com.adcoretechnologies.golfbudz.LauncherActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.adcoretechnologies.golfbudz.home.SplashScreen"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.home.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.home.WelcomeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.adcoretechnologies.golfbudz.auth.profile.ProfileActivity" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.auth.profile.EditProfileActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.adcoretechnologies.golfbudz.auth.profile.ChnagePasswordActivity" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.items.BuyItemActivity"
            android:label="Buy Items" />
        <activity android:name="com.adcoretechnologies.golfbudz.items.BuyItemDetailActivity" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.auth.login.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.auth.Register.RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name="com.adcoretechnologies.golfbudz.more.MoreActivity" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.notification.NotificationActivity"
            android:label="Notifications" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.friends.MyFriendActivity"
            android:label="Search Friends" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.event.EventActivity"
            android:label="Events" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.event.CreateEventActivity"
            android:label="Create Event" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.club.MyClubActivity"
            android:label="Golf Clubs" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.club.ClubDetailActivity"
            android:label="Detail" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.group.CreatePageActivity"
            android:label="Create Group" />
        <activity android:name="com.adcoretechnologies.golfbudz.chat.ChatDashboradActivity" />
        <activity android:name="com.adcoretechnologies.golfbudz.auth.login.ForgotActivity" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.club.JoinClubActivity"
            android:label="Join Club" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.items.SellItemActivity"
            android:label="Post an item for sale" />
        <activity android:name="com.adcoretechnologies.golfbudz.home.ImageDataPostActivity" />
        <activity android:name="com.adcoretechnologies.golfbudz.blog.BLogActivity" />
        <activity android:name="com.adcoretechnologies.golfbudz.blog.OnCourseActivity" />
        <activity android:name="com.adcoretechnologies.golfbudz.blog.BlogDeatilActivity" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.playrequest.PlayRequestActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.adcoretechnologies.golfbudz.playrequest.RequestDetailActivity" />
        <activity android:name="com.adcoretechnologies.golfbudz.event.EventDetailActivity" />
        <activity android:name="com.adcoretechnologies.golfbudz.home.VideoDatapostActivity" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.more.AboutUsActivity"
            android:label="About Us" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.more.ContactUsActivity"
            android:label="Contact Us" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.more.TermsConditionActivity"
            android:label="Terms and Condition" />
        <activity android:name="com.adcoretechnologies.golfbudz.home.VideoActivity" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.friends.FriendProfileActivity"
            android:label="Profile" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.chat.MyChatActivity"
            android:label="Budz chat" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.home.comment.CommentsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.group.invite.InviteFriendActivity"
            android:label="Add Member" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.group.MyGroupActivity"
            android:label="My Groups" />
        <activity
            android:name=".playrequest.UpComingGanesDetailActivity"
            android:label="Details" />
        <activity
            android:name="com.adcoretechnologies.golfbudz.friends.MyAllPlayReqActivity"
            android:label="My Play Request" />

        <service android:name="com.adcoretechnologies.golfbudz.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.adcoretechnologies.golfbudz.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity android:name="com.adcoretechnologies.golfbudz.home.gallery.GalleryActivity" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
    </application>

</manifest>